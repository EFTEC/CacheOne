    "redis",
    "php",
    "cache",
    "file system",
    "pdo"
  ],
  "homepage": "https://github.com/EFTEC/CacheOne",
  "authors": [
    {
      "name": "Jorge Castro Castillo",
      "homepage": "https://github.com/EFTEC/"
    }
  ],
